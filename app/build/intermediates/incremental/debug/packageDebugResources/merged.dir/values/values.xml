    <style name="ACButton" parent="BaseButton">
        <item name="android:background">@drawable/ac_button_background</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="BaseButton" parent="Widget.AppCompat.Button">
        <item name="android:layout_margin">2dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="android:elevation">4dp</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:padding">4dp</item>
        <item name="android:minWidth">48dp</item>
        <item name="android:minHeight">48dp</item>
    </style>
    <style name="BaseButtonStyle">
        <item name="android:layout_margin">2dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">8dp</item>
        <item name="android:elevation">2dp</item>
    </style>
    <style name="DeleteButton" parent="BaseButton">
        <item name="android:background">@drawable/delete_button_background</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>
    <style name="EmptyCell">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_columnWeight">1</item>
        <item name="android:layout_rowWeight">1</item>
    </style>
    <style name="EqualsButton" parent="BaseButton">
        <item name="android:background">@drawable/equals_button_background</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="FunctionButton" parent="BaseButton">
        <item name="android:background">@drawable/function_button_background</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="MemoryButton" parent="BaseButton">
